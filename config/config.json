{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hola-mundo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/holaMundo/HolaMundoWebPart.js",
          "manifest": "./src/webparts/holaMundo/HolaMundoWebPart.manifest.json"
        }
      ]
    },
    "prueba-property-pane-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pruebaPropertyPane/PruebaPropertyPaneWebPart.js",
          "manifest": "./src/webparts/pruebaPropertyPane/PruebaPropertyPaneWebPart.manifest.json"
        }
      ]
    },
    "prueba-pn-p-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pruebaPnP/PruebaPnPWebPart.js",
          "manifest": "./src/webparts/pruebaPnP/PruebaPnPWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HolaMundoWebPartStrings": "lib/webparts/holaMundo/loc/{locale}.js",
    "PruebaPropertyPaneWebPartStrings": "lib/webparts/pruebaPropertyPane/loc/{locale}.js",
    "PruebaPnPWebPartStrings": "lib/webparts/pruebaPnP/loc/{locale}.js"
  }
}
